# show envs passed to the service (runs a one-off container, won't execute entrypoint)
docker compose run --rm --entrypoint env wren-ai-service | egrep -i 'WREN_AI_SERVICE_PORT|QDRANT|CLICKHOUSE' || true

# check qdrant reachability from the image
docker compose run --rm --entrypoint sh wren-ai-service -c 'nc -zq2 qdrant 6333 && echo qdrant_ok || echo qdrant_fail'
